{
  "name": "phpj/phpj",
  "type": "library",
  "description": "PHP OOP Core inspired by Java",
  "keywords": ["Java", "OOP", "Objects"],
  "homepage": "http://github.com/phpj/phpj",
  "license": "MIT",

  "authors": [
    {
      "name": "Yury Kozyrev",
      "email": "urakozz@gmail.com",
      "homepage": "https://github.com/urakozz"
    }
  ],

  "require": {
    "php": ">=5.6",
    "ext-intl": "*",
    "phpoption/phpoption":"*",
    "kozz/static-cache":"*"
  },

  "autoload": {
    "psr-4": {
      "PHPJ\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PHPJ\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "~4.2"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },

  "minimum-stability": "stable"
}
