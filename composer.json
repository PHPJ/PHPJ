{
  "name": "phpj/phpj",
  "type": "library",
  "description": "PHP OOP Core inspired by Java",
  "keywords": ["Java", "OOP", "Objects"],
  "homepage": "http://github.com/phpj/phpj",
  "license": "MIT",
  "authors": [
    {
      "name": "Yury Kozyrev",
      "email": "urakozz@gmail.com",
      "homepage": "https://github.com/urakozz"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "ext-intl": "*",
    "ext-gmp": "*",
    "phpoption/phpoption": "~1.4",
    "symfony/filesystem": "~2.6",
    "kozz/static-cache": "~0.1"
  },
  "autoload": {
    "psr-4": {
      "PHPJ\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PHPJ\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "~4.2"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "stable"
}
